    <div class="row no-margin flex-center"> 
        <div class="col element-data element-name s12 m{{ 12/(nbFields + 1) }}" style="font-size: 17px;display: none;">
        </div>
        <div class="input-field element-input col no-margin s12 m{{ 12/(nbFields + 1) }}">
            {{ form_row(item.name) }}
        </div>
        {% if elmtType == 'department' %}
            <div class="col element-data element-master-user s12 m{{ 12/(nbFields + 1) }}" style="font-size: 17px;display: none;">
            </div>
            <div class="col element-input s12 m{{ 12/(nbFields + 1) }}">
                {{ form_row(item.masterUser) }}
            </div>
        {% endif %}
        <div class="col s12 m{{ 12/(nbFields + 1) }}">
            <ul class="flex-center">
                <li style="margin-left:auto;margin-right:10px"><a class="validate waves-effect waves-light btn blue darken-2 tooltip"><i class="fa fa-check"></i></a></li>
                <li style="margin-right:10px;display:none">
                    <a class="waves-effect waves-light btn-flat tooltipped" data-position="top" data-tooltip="{% trans with {'%conj_element%' : elmtNameConj} %}organization_elements.no_users{% endtrans %}" style="width:75px">
                    {#<i class="fa fa-users"></i>#}
                        <div style="position:relative;display:inline-block">
                            <i class="fa fa-users"></i>
                            <div class="bar"></div>
                        </div>
                    </a>
                </li>
                <li style="margin-right:10px;display:none;"><a class="validate waves-effect waves-light btn tooltipped" href="{{ url('updateElementTargets',{'entity':  , 'elmtId' : 0}) }}" data-position="top" data-tooltip="{% trans %}organization_elements.targets{% endtrans %}"><i class="fa fa-bullseye"></i></a></li>
                <li><a class="waves-effect waves-light accent-4 btn-flat tooltip remove-element" data-eid="0" data-tooltip="{% trans %}organization_elements.delete_btn{% endtrans %}"><i class="fa fa-trash"></i></a></li>
            </ul>
          </div>
    </div>
    
